#include <stdio.h>

int main()
{
    int i,j,k,a[100],elements,temp;
    printf("Enter the number of elements ");
    scanf("%d",&elements);
    printf("Enter the elements in unsorted manner\n");
    for(i=0;i<elements;i++)
    scanf("%d",&a[i]);
    int step,count;
    for(j=0;j<elements;j++)
    {
        for(k=j+1;k<elements-1;k++)
        {
            if(a[j]>a[k])
            {
                temp= a[j];
                a[j]=a[k];
                a[k]=temp;
            }
        }
    }
    printf("Sorted array is ");
    for(i=0;i<elements;i++)
    printf("%d ",a[i]);
    
    return 0;
}
